local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "OPZTVBOOM's Private GUI", HidePremium = false, IntroText = "OPZTVBOOM's Gui", SaveConfig = true, ConfigFolder = "OrionTest"})

local tablewhitelistusernames = {
    "OPZTVBOOM",
    "JohnMark201745",
    "",
    "" 
}
local tablewhitelistuserids = {
    IDHERE,
    IDHERE,
    IDHERE,
    IDHERE 
}
local whitelist = tablewhitelistusernames
local whitelistids = tablewhitelistuserids
local plr = game:GetService("Players").LocalPlayer
if not table.find(whitelist, plr.Name) and not table.find(whitelistids, plr.UserId) then
    plr:Kick("You are not whitelisted! Ask the owner (OPZTVBOOM) to be whitelisted.")
end

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "COMING SOON",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

